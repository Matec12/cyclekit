{
  "expo": {
    "name": "cyclekit",
    "slug": "cyclekit",
    "version": "1.0.0",
    "scheme": "cyclekit",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/logo-cyclekits.png",
      "backgroundColor": "#7b091c"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logo-cyclekits.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.zeeohn.cyclekit"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "extra": {
      "router": {
        "origin": false
      }
    }
  }
}
